<%= link_to "Edit", edit_question_path(@question), class: 'btn btn-success' %>
<%= button_to "Delete", question_path(@question), class: 'btn btn-danger', method: "delete", data: { confirm: 'Are you wheelwe sure?', disable_with: 'loading...' } %>
<h1><%= @question.title %></h1>
<p><%= @question.body %></p>
<p>Resolved? <%= @question.resolved %> </p>